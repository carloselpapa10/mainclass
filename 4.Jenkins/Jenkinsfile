pipeline {
    agent none
    tools { 
        maven 'maven-3' 
    }
    stages {
	stage('Maven Clean Package') {
		agent any
	     	steps {
			sh 'mvn -f "4.Jenkins/order-service/pom.xml" -B -DskipTests clean package docker:build'
	      	}
	} 
	stage('Docker Compose') {
	      agent any
	      steps {
		sh 'docker-compose -f 4.Jenkins/docker-compose.yml up'
	      }
	}
    }
}
